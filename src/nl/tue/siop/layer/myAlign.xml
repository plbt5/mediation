<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE rdf:RDF [
<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
<!ENTITY wine "http://www.w3.org/TR/2003/CR-owl-guide-20030818/wine#">
<!ENTITY vin "http://ontology.deri.org/vin#">
<!ENTITY proton "http://proton.semanticweb.org/">
<!ENTITY edoal "http://ns.inria.org/edoal/1.0/#">
]>

<!-- JE2010: With the "#" in the end, this is parsed (by the RDF parser...)
without it, this is not! -->
<rdf:RDF xmlns="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#" 
         xml:base="http://oms.omwg.org/ontoA-ontoB/"
         xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#"
         xmlns:alext="http://exmo.inrialpes.fr/align/ext/1.0/"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:ex="http://example.org"
         xmlns:proton="http://proton.semanticweb.org/"
         xmlns:edoal="http://ns.inria.org/edoal/1.0/#" xmlns:ontoA="file:///ontoA.rdf#" xmlns:ontoB="file:///ontoB.ttl#">
  
    <Alignment rdf:about="http://oms.omwg.org/ontoA-ontoB/"> 
        <xml>yes</xml>
        
        <dc:creator>PaulBrandt</dc:creator><dc:date>2015/08/25</dc:date>
        <alext:method>manual</alext:method>
        <alext:purpose>initial example for a simple alignment</alext:purpose>
        <level>2EDOAL</level>
        <type>?*</type>    
        <onto1>
            <Ontology rdf:about="&amp;ontoA;">
                <align:location>file:ontoA.rdf</align:location><formalism>
                    <Formalism align:name="OWL1.0" align:uri="http://www.w3.org/2002/07/owl#"/>
                </formalism>
                
            </Ontology>
        </onto1> 
        <onto2>
            <Ontology rdf:about="&amp;ontoB;">
                <location>file:ontoB.ttl</location>
                <formalism>
                    <Formalism align:uri="http://www.w3.org/2008/turtle#" align:name="turtle" />
                </formalism>
            </Ontology>
        </onto2>
        <!-- Class basis -->
        <map>
            <Cell rdf:about="MappingRule_class_equiv">
                <entity1>
                    <edoal:Class rdf:about="&amp;ontoA;OneEq"/>
                </entity1>
                <entity2>
                    <edoal:Class rdf:about="&amp;ontoB;OneEq"/>
                </entity2>
                <measure rdf:datatype="&xsd;float">1.0</measure>
                <relation>=</relation>
            </Cell>
        </map>
    </Alignment>
</rdf:RDF>
