<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE rdf:RDF [
<!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
<!ENTITY ontoA	"file:///ontoA.rdf/">
<!ENTITY ontoB	"file:///ontoB.rdf/">
<!ENTITY proton "http://proton.semanticweb.org/">
]>

<!-- JE2010: With the "#" in the end, this is parsed (by the RDF parser...) 
	without it, this is not! -->
<rdf:RDF xmlns="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#"
	xml:base="http://oms.omwg.org/ontoA-ontoB/" 
	xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
	xmlns:align="http://knowledgeweb.semanticweb.org/heterogeneity/alignment#"
	xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:ex="http://example.org" 
	xmlns:ontoA="file:///ontoA.rdf#"
	xmlns:ontoB="file:///ontoB.rdf#"
	xmlns:proton="http://proton.semanticweb.org/"
	xmlns:edoal="http://ns.inria.org/edoal/1.0/#">

	<Alignment rdf:about="http://oms.omwg.org/ontoA-ontoB/">
		<xml>yes</xml>

		<dc:creator>PaulBrandt</dc:creator>
		<dc:date>2015/08/25</dc:date>
		<method>manual</method>
		<purpose>initial example for a simple alignment</purpose>
		<level>2EDOAL</level>
		<type>?*</type>
		<onto1>
			<Ontology rdf:about="&amp;ontoA;">
				<align:location>file:ontoA.rdf</align:location>
				<formalism>
					<Formalism align:name="OWL1.0" align:uri="http://www.w3.org/2002/07/owl#" />
				</formalism>

			</Ontology>
		</onto1>
		<onto2>
			<Ontology rdf:about="&amp;ontoB;">
				<location>file:ontoB.rdf</location>
				<formalism>
					<Formalism align:uri="http://www.w3.org/2002/07/owl#"
						align:name="OWL1.0" />
				</formalism>
			</Ontology>
		</onto2>
		<!-- Class basis -->
		<map>
			<Cell rdf:about="MappingRule_0">
				<entity1>
					<edoal:Class rdf:about="&amp;ontoA;OneEq" />
				</entity1>
				<entity2>
					<edoal:Class rdf:about="&amp;ontoB;OneEq" />
				</entity2>
				<measure rdf:datatype="&xsd;float">1.0</measure>
				<relation>Equivalence</relation>
			</Cell>
		</map>
	</Alignment>
</rdf:RDF>
